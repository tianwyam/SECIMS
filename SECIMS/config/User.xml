<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cuit.secims.mw.dao.IUserDao">
	<select id="findUserByName"	parameterType="String" resultType="com.cuit.secims.xp.domin.User">
		select * from user where name like '%${value}%'
	</select>
	
	
	<select id="getAll" resultType="User">
		<![CDATA[
			SELECT * FROM USER
		]]>
	</select>
</mapper>