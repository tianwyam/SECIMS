<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<!-- 配置别名 -->
	<typeAliases>
		<typeAlias type="com.cuit.secims.mw.pojo.User" alias="User"  />
		<typeAlias type="com.cuit.secims.mw.pojo.StudyPlan" alias="StudyPlan"  />
		<typeAlias type="com.cuit.secims.mw.pojo.StudyPlanDetail" alias="StudyPlanDetail"  />
		<typeAlias type="com.cuit.secims.mw.pojo.MonthReview" alias="MonthReview"  />
		<typeAlias type="com.cuit.secims.mw.pojo.WeekReportWork" alias="WeekReportWork"  />
		<typeAlias type="com.cuit.secims.mw.pojo.LearningForumPosts" alias="LearningForumPosts"  />
	</typeAliases>
	
	
	
	<!-- 配置的映射文件 -->
	<mappers>
		<mapper resource="User.xml"/>
		<mapper resource="mybatis/mapper/StudyPlanMapper.xml"/>
		<mapper resource="mybatis/mapper/StudyPlanDetailMapper.xml"/>
		<mapper resource="mybatis/mapper/MonthReviewMapper.xml"/>
		<mapper resource="mybatis/mapper/WorkReportMapper.xml"/>
		<mapper resource="mybatis/mapper/LearningForumMapper.xml"/>
	</mappers>
	
	
	
</configuration>